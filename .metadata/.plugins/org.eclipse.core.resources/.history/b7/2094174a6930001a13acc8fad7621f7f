#define CHUNK 1024 /* read 1024 bytes at a time */
#include <stdio.h>
#include<string.h>

int main(){
	char buf[CHUNK];
	FILE *file;
	size_t nread;

	file = fopen("file_read.txt", "r");
	if (file) {
		while ((nread = fread(buf, 1, sizeof buf, file)) > 0)
			printf("%d\n",strcmp(buf,"<TEXT>"));
			if(strcmp(buf,"<TEXT>"))
				fwrite(buf, 1, nread, stdout);
		if (ferror(file)) {
			printf("Error reading file\n");
		}
		fclose(file);
	}
}
